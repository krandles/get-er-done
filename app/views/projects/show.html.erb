<h2>Project Name: <%= @project.name %></h2>
<h3>Status: <%= @project.status ? "Complete" : "Incomplete" %></h3>
<h3>Points for Completion <%= @project.points %></h3>
<br>
<h3>Project Tasks</h3>
<ul>
  <% @project.tasks.each do |task| %>
    <li><%= link_to task.name, task %></li>
  <% end %>
</ul>
<br>
<%= button_to "Edit Project", edit_project_path(@project.id), method: :get %>
<br>
<%= button_to "Delete Project", @project, method: :delete %>
