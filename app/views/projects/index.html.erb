<div class="row">
  <div class="col-sm-6">
    <h3>Incomplete Projects</h3>
    <ul>
      <% @projects.each do |project| %>
        <% if !project.complete %>
          <li><%= link_to project.name, project %></li>
        <% end %>
      <% end %>
    </ul>
  </div>
  <div class="col-sm-6"></div>
    <h3>Complete Projects</h3>
    <ul>
      <% @projects.each do |project| %>
        <% if project.complete %>
          <li><%= link_to project.name, project %></li>
        <% end %>
      <% end %>
    </ul>
  </div>
<%= button_to "Add Project", new_project_path, method: :get, class: 'btn btn-primary' %>
</div>
