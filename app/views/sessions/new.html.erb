<%= form_for @user, url: login_path, method: :post do |f| %>
  <%= f.label :user_name %>
  <%= f.text_field :user_name %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Log in" %>
<% end %>
New user? <%= link_to "Create an account", new_user_path %>
<%= params.inspect %>